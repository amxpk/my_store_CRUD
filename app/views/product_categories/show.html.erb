
<table>
  <thead>
    <th>Id</th>
    <th>Name</th>
    <th>Created_at</th>
    <th>Updated_at</th>
  </thead>
  <%  @productC.products.each do |f| %>
  <tr>
    <td><%= f.id %> </td>
    <td><%=  f.name %>  </td>
    <td><%= f.created_at %></td>
    <td><%= f.updated_at %></td>
    <td><%= link_to "Edit", edit_product_path(f) %></td>
    <td><%= link_to "Delete",f,method: :delete, data:{confrim:"Are you sure ?"} %></td>
</tr>
    <% end %>
</table>
